{
  "expo": {
    "name": "RafiNewApp",
    "slug": "RafiNewApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconnew.png",
    "scheme": "rafinewapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manishdhakecha.rafinewapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["instagram"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconnew.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rafi202.RafiNewApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconnew.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59ba58c2-0fc2-4c91-a387-bf1db90cd0e1"
      }
    },
    "owner": "princi607"
  }
}
